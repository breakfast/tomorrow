<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with type="com.breakfast.tomorrow.web.client.ApplicationConstants"
		field="cc">
	</ui:with>
	
	

	<ui:style>
		.application-menu-css {
			width: 250px;
			padding-left: 10px;
			padding-top: 10px;
		}
		
		.stack-group-menu-css {
			width: 250px;
		}
		
		.item-menu-css {
			background: #404040;
			background : rgba(241,241,241,0.8);
			cursor: pointer;
			height: 30px;
			width: 250px;
			padding: 5px 7px;
			height: 30px;
			border-top-width: thin;
			border-top-color: gray;
			border-top-color: white;
			border-top-style: solid;  
		}
		
		.item-menu-css a{
			color: white;
			color: #101010;
		}
		
		.item-menu-css:HOVER {
			background: #101010;
			background: gray;
		}
		
}
	</ui:style>

	<g:HTMLPanel stylePrimaryName="{style.application-menu-css}" >
		<g:VerticalPanel ui:field="menuPanel">
			
			<g:Hyperlink stylePrimaryName="{style.item-menu-css}" ui:field="cadastrarCurso" targetHistoryToken="matricular"></g:Hyperlink>
			<g:Hyperlink stylePrimaryName="{style.item-menu-css}" ui:field="matricularAluno" targetHistoryToken="outro"></g:Hyperlink>
			<g:Hyperlink stylePrimaryName="{style.item-menu-css}" ui:field="aluno" targetHistoryToken="alunos"></g:Hyperlink>
			<g:Hyperlink stylePrimaryName="{style.item-menu-css}" ui:field="professor"></g:Hyperlink>
			<g:Hyperlink stylePrimaryName="{style.item-menu-css}" ui:field="turma"></g:Hyperlink>
			<g:Hyperlink stylePrimaryName="{style.item-menu-css}" ui:field="curso" targetHistoryToken="cursos"></g:Hyperlink>
			<g:Hyperlink stylePrimaryName="{style.item-menu-css}" ui:field="configuracoes"></g:Hyperlink>
			
			
			
