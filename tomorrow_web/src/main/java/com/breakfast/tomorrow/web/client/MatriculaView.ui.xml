<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	>
	<ui:with type="com.breakfast.tomorrow.web.client.ApplicationConstants"
		field="cc">
	</ui:with>
	<g:HTMLPanel styleName="content-page">
		<g:Label text="{cc.matricularAluno}" styleName="app-title">
		</g:Label>
		<hr />
		<g:Label text="{cc.aluno}" ui:field="aluno"></g:Label>
		<br/>
		<table>
			<tr>
				<td>
					<g:Label text="{cc.curso}"> </g:Label>
				</td>
				<td>
					<g:ListBox width="400px" ui:field="curso"></g:ListBox>
				</td>
			</tr>
			<tr>
				<td>
					<g:Label text="{cc.turma}"> </g:Label>
				</td>
				<td>
					<g:ListBox width="400px" ui:field="turma"></g:ListBox>		
				</td>
			</tr>
			<tr>
				<td>
					<g:Label text="{cc.etapa}"> </g:Label>
				</td>
				<td>
					<g:ListBox width="400px" ui:field="etapa"></g:ListBox>		
				</td>
			</tr>
		</table>
		<br/>
		<hr/>
		<g:HorizontalPanel>
			<g:Button text="{cc.matricular}" ui:field="matricular"></g:Button>
		</g:HorizontalPanel>
			
	</g:HTMLPanel>
	
</ui:UiBinder> 